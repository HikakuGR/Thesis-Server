@model IEnumerable<Coordination.Model.JobAssignment>


@foreach (Coordination.Model.JobAssignment assignment in Model)
{
    <div class="row">
               
            <div class="col-md-7">
                <h1>@assignment.User.Username is assigned to @assignment.Job.JobName (@assignment.Job.ID.ToString())</h1>
            </div>
                

        
    </div>
}



